{
  "name": "api-document-generator",
  "version": "1.0.0",
  "description": "Multi-runtime CLI tool for automated API documentation generation",
  "main": "dist/cli/index.js",
  "bin": {
    "api-doc-gen": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "api",
    "documentation",
    "generator",
    "cli",
    "openapi",
    "swagger",
    "typescript",
    "ai"
  ],
  "author": "API Documentation Generator Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/axios": "^0.9.36",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "ajv": "^8.12.0",
    "axios": "^1.12.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "doctrine": "^3.0.0",
    "express": "^5.1.0",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.1.1",
    "globby": "^11.1.0",
    "graphql": "^16.11.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "inquirer": "^8.2.6",
    "morgan": "^1.10.1",
    "openai": "^4.20.1",
    "swagger-parser": "^10.0.3",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/inquirer": "^9.0.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  }
